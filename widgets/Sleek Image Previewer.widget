{
  "version": "1.0",
  "name": "Sleek Image Previewer",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"background\":{{ (glass) | tojson }},\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{\"type\":\"Image\",\"src\":{{ (image.src) | tojson }},\"alt\":{{ (image.alt) | tojson }},\"aspectRatio\":{{ ((16 / 9)) | tojson }},\"radius\":\"xl\",\"frame\":true,\"flush\":true,\"children\":[]},{\"type\":\"Row\",\"align\":\"center\",\"gap\":3,\"children\":[{\"type\":\"Col\",\"gap\":0,\"flex\":\"auto\",\"children\":[{\"type\":\"Text\",\"value\":{{ (fileName) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\",\"maxLines\":1,\"children\":[]},{\"type\":\"Caption\",\"value\":{{ (meta) | tojson }},\"color\":\"secondary\",\"children\":[]}]},{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Button\",\"label\":\"Download\",\"style\":\"primary\",\"onClickAction\":{\"type\":\"file.download\",\"payload\":{\"url\":{{ (image.src) | tojson }},\"name\":{{ (fileName) | tojson }}}},\"children\":[]},{\"type\":\"Button\",\"label\":\"Share\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"file.share\",\"payload\":{\"url\":{{ (image.src) | tojson }},\"name\":{{ (fileName) | tojson }}}},\"children\":[]}]}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "image": {
        "type": "object",
        "properties": {
          "src": {
            "type": "string"
          },
          "alt": {
            "type": "string"
          }
        },
        "required": [
          "src",
          "alt"
        ],
        "additionalProperties": false
      },
      "fileName": {
        "type": "string"
      },
      "meta": {
        "type": "string"
      },
      "glass": {
        "type": "object",
        "properties": {
          "light": {
            "type": "string"
          },
          "dark": {
            "type": "string"
          }
        },
        "required": [
          "light",
          "dark"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "image",
      "fileName",
      "meta",
      "glass"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "md",
    "background": {
      "light": "rgba(255,255,255,0.55)",
      "dark": "rgba(20,20,20,0.45)"
    },
    "children": [
      {
        "type": "Col",
        "gap": 3,
        "children": [
          {
            "type": "Image",
            "src": "https://upload.wikimedia.org/wikipedia/commons/4/48/Gradient_Abstract_Background.jpg",
            "alt": "Gradient abstract background",
            "aspectRatio": 1.7777777777777777,
            "radius": "xl",
            "frame": true,
            "flush": true
          },
          {
            "type": "Row",
            "align": "center",
            "gap": 3,
            "children": [
              {
                "type": "Col",
                "gap": 0,
                "flex": "auto",
                "children": [
                  {
                    "type": "Text",
                    "value": "Gradient_Abstract_Background.jpg",
                    "size": "sm",
                    "weight": "semibold",
                    "maxLines": 1
                  },
                  {
                    "type": "Caption",
                    "value": "JPEG • 4680×3000 • CC0",
                    "color": "secondary"
                  }
                ]
              },
              {
                "type": "Row",
                "gap": 2,
                "children": [
                  {
                    "type": "Button",
                    "label": "Download",
                    "style": "primary",
                    "onClickAction": {
                      "type": "file.download",
                      "payload": {
                        "url": "https://upload.wikimedia.org/wikipedia/commons/4/48/Gradient_Abstract_Background.jpg",
                        "name": "Gradient_Abstract_Background.jpg"
                      }
                    }
                  },
                  {
                    "type": "Button",
                    "label": "Share",
                    "variant": "outline",
                    "onClickAction": {
                      "type": "file.share",
                      "payload": {
                        "url": "https://upload.wikimedia.org/wikipedia/commons/4/48/Gradient_Abstract_Background.jpg",
                        "name": "Gradient_Abstract_Background.jpg"
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
